apiVersion: extensions/v1beta1
kind: Deployment
metadata:
  name: nginx-deployment-k8s-test-new
  labels:
    app: nginx
spec:
  replicas: 1
  selector:
    matchLabels:
      app: nginx
  template:
    metadata:
      labels:
        app: nginx
    spec:
      containers:
      - name: tomcat
        image: tomcat:8
        ports:
        - containerPort: 80
